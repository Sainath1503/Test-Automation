<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Test Automation Framework Suite"
       parallel="methods"
       thread-count="4"
       data-provider-thread-count="4">
    <listeners>
        <listener class-name="com.automention.framework.listeners.ReportSummaryListener"/>
    </listeners>
    <test name="Cucumber Tests">
        <classes>
            <class name="com.automention.framework.runners.TestRunner"/>
        </classes>
    </test>
</suite>
